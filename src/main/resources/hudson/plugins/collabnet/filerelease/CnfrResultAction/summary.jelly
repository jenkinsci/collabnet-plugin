<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <t:summary icon="${it.getSummaryIcon()}">
    <j:choose>
      <j:when test="${it.isSuccess()==true}">
        Successfully uploaded ${it.getNumFiles()} file(s) to the <a href="${it.getCnfrUrlName()}">CollabNet File Release System</a>.
      </j:when>
      <j:otherwise>
        CollabNet File Release: Failed to upload any files.
      </j:otherwise>
      (See <a href="${it.getResultUrlName()}">console</a> for details.)
     </j:choose>
  </t:summary>
</j:jelly>
